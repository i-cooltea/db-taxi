server:
  port: 8080
  host: "0.0.0.0"
  read_timeout: "30s"
  write_timeout: "30s"
  enable_https: false
  cert_file: ""
  key_file: ""

database:
  host: "localhost"
  port: 3306
  username: "root"
  password: "your_password_here"
  database: "your_database_name"
  ssl: false
  max_open_conns: 25
  max_idle_conns: 5
  conn_max_lifetime: "5m"
  query_timeout: "30s"

security:
  session_timeout: "30m"
  read_only_mode: false
  enable_audit: true

logging:
  level: "info"
  format: "json"
  output: "stdout"
  max_size: 100
  max_backups: 3
  max_age: 28